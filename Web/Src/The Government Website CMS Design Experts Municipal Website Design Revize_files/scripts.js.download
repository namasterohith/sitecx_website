(function($) {

	'use strict';

	var $window = $(window),
		$body = $('body');

	/*!
	 * IE10 viewport hack for Surface/desktop Windows 8 bug
	 * Copyright 2014-2015 Twitter, Inc.
	 * Licensed under MIT (https://github.com/twbs/bootstrap/blob/master/LICENSE)
	 */

	// See the Getting Started docs for more information:
	// http://getbootstrap.com/getting-started/#support-ie10-width
	if (navigator.userAgent.match(/IEMobile\/10\.0/)) {
		var msViewportStyle = document.createElement('style');
		msViewportStyle.appendChild(
			document.createTextNode(
			  '@-ms-viewport{width:auto!important}'
			)
		); document.querySelector('head').appendChild(msViewportStyle);
	}

	// Preloader
	$window.load(function() {
		setTimeout(function(){
			$body.addClass('loaded form-stick');
			 $('#loader-wrapper').fadeOut();
		}, 600);
	});

	if($(".tour").length>0){
		$(window).on("load", function (e) {
		var window_height = $(window).height();
		var window_height_half = window_height * 3/4;
		var window_srollTop = $(window).scrollTop() + window_height_half;
		var window_srollTopFull = $(window).scrollTop() + window_height;
		//var tour_footer_offset = $(".tour_footer_holder").offset().top;
		$(window).resize(function(){
				window_height = $(window).height();
				window_height_half = window_height * 3/4;
		});
		$(window).scroll(function(){
				window_srollTop = $(window).scrollTop() + window_height_half;
				window_srollTopFull = $(window).scrollTop() + window_height;
				$(".animatable").each(function(){
						if(window_srollTop >= $(this).offset().top){
								$(this).addClass("animated");
						}
				});
/*          if(window_srollTopFull >= tour_footer_offset){
						$(".tour_footer").css("display","block");
				} */
		});
		$(window).trigger("scroll");
		});
	}

	$('.project-feed').infiniteScroll({
		path: '.pagination__next',
		append: '.project',
		status: '.scroller-status',
		hideNav: '.pagination',
	});

	// Add validation parts
	$('#contact input[type=text], #contact input[type=number], #contact input[type=email], #contact input[type=tel], #contact select, #contact textarea').each(function(){
		$(this).after('<mark class="validate"></mark>');
	});

	$('#name').focus('input', function(e) {
		$('#contact-toggle').show(600).delay();
		$body.addClass('is-typing');
		$body.removeClass('form-stick');
	});

	$('#contact-form-close').on('click',function(e) {
		$('#contact-toggle').delay().hide(300);
		setTimeout(function(){
			$body.removeClass("is-typing");
			$body.addClass("form-stick");

		}, 600);

	});

	$('.open-contact-form').click(function(e){
		$('#name').focus();
			$('html, body').animate({scrollTop: '+=301px'}, 600);
		e.preventDefault();
		return false;
/*          $body.removeClass("form-stick"); */
	});


	// Validate as you type
	$('#contactform input').each(function(){
		$(this).after('<mark class="validate"></mark>');
	});
	$('#name, #comments, #subject').focusout(function() {
		if (!$(this).val())
			$(this).addClass('error').parent().find('mark').removeClass('valid').addClass('error');
		else
			$(this).removeClass('error').parent().find('mark').removeClass('error').addClass('valid');
	});
	$('#email').focusout(function() {
		if (!$(this).val() || !isEmail($(this).val()))
			$(this).addClass('error').parent().find('mark').removeClass('valid').addClass('error');
		else
			$(this).removeClass('error').parent().find('mark').removeClass('error').addClass('valid');
	});
	$('#website').focusout(function() {
		var web = $(this).val();
		if (web && web.indexOf("://") == -1) {
			$(this).val('http://' + web);
			$(this).removeClass('error').parent().find('mark').removeClass('error').addClass('valid');
		} else if (web)
			$(this).removeClass('error').parent().find('mark').removeClass('error').addClass('valid');
		else
			$(this).removeClass('error').parent().find('mark').removeClass('error').removeClass('valid');
	});

	function contactFormSubmit() {

		var postData,
			$submit = $('#submit'),
			$message = $('#contact-results'),
			$formElement = $('#contact-body'),
			$scrollElement = $('#contact-form');


		if( $('#contactform').length > 0 ) {
			$formElement = $("#contactform");
			postData = $formElement.serialize();
			$message = $('#message'),
			$scrollElement = $('html, body');
			if ($('#contact mark.error').size()>0) {
				return false;
			}
		} else {
			postData = {
                'name'          : $('input[name=name]').val(),
                'email'         : $('input[name=email]').val(),
                'phone'         : $('input[name=phone]').val(),
                'subject'       : $('select[name=subject]').val(),
                'comments'      : $('textarea[name=comments]').val()
            };
		}

 		$submit
			.after('<img src="html5-contact/assets/img/ajax-loader.gif" class="loader" />')
			.attr('disabled','disabled');

		$.post("html5-contact/classes/contact.php", postData,
			function(data){
				$submit.removeAttr('disabled');
				if( data.type == 'success') {
					console.log("success", data.text );
					$message
						.html( data.text )
						.slideDown();
					$('#contactform img.loader').fadeOut('slow',function(){$(this).remove()});
					$formElement.slideUp('slow');
					// window.location.href = "thank-you";
				} else if( data.type == 'error' ) {
					console.log("error", data.text );
					$message
						.html( data.text )
						.slideDown();
					if( $scrollElement.attr('id') == 'contact-form' ) {
						$scrollElement.animate({
					        scrollTop: 0
					    }, 400);
					}
				}
			}, "json"
		);

		return false;
	};

	function isEmail(emailAddress) {
		var pattern = new RegExp(/^(("[\w-\s]+")|([\w-]+(?:\.[\w-]+)*)|("[\w-\s]+")([\w-]+(?:\.[\w-]+)*))(@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$)|(@\[?((25[0-5]\.|2[0-4][0-9]\.|1[0-9]{2}\.|[0-9]{1,2}\.))((25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\.){2}(25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\]?$)/i);
		return pattern.test(emailAddress);
	}

	function isNumeric(input) {
    	return (input - 0) == input && input.length > 0;
	}

	// Contact-form Submissions
	$('#contactform').submit(contactFormSubmit);
	$('#submit_btn').click(contactFormSubmit);

	$('#verify').focusout(function() {
		var verify = $(this).val();
		var verify_box = $(this);
		if (!verify)
			$(this).addClass('error').parent().find('mark').removeClass('valid').addClass('error');
		else {

			// Test verification code via ajax
			$.ajax({
				type: 'POST',
				url: 'html5-contact/classes/ajax_check.php',
				data: { verify: verify },
				async: false,
				success: function( data ) {
					if (data=='success') {
						$(verify_box).removeClass('error').parent().find('mark').removeClass('error').addClass('valid');
					} else {
						$(verify_box).addClass('error').parent().find('mark').removeClass('valid').addClass('error');
					}
				}
			});

		}
	});

	$window.ready(function(){
			if($(".rellax").length>0){
				var rellax = new Rellax('.rellax');
			}

			$('#mc-form').ajaxChimp({
				url: 'https://revize.us17.list-manage.com/subscribe/post?u=0b131ee1159eb71cd8d23214d&amp;id=41b57f18c8'
			});

			//forms
	  $(".form-control").focus(function(){
		   $(this).parent().addClass('typing');

	  }).blur(function(){
		   $(this).parent().removeClass('typing');
	  })

		$('.form-control').on('keydown', function(){
		   if($(this).val().length){
			 $(this).parent().addClass('filled');
		   }
		});

		 $('#mega-drop-toggle').on('click',function(e){
			$body.toggleClass('menu-drop-open').stop();
			$(this).toggleClass('active');
			$('nav').slideToggle(300);
